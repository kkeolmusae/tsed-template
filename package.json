{
  "name": "tsed-template",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "jest",
    "build": "yarn run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "yarn run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:dev": "nodemon --watch src -e ts,js,mjs --ignore ‘node_modules/**/*’ --exec NODE_ENV=development ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/db/DefaultDataSource.ts",
    "migration:generate": "yarn typeorm migration:generate",
    "migration:create": "yarn typeorm migration:create",
    "migration:run": "yarn typeorm migration:run",
    "migration:run:dev": "cross-env NODE_ENV=development yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "migration:show": "yarn typeorm migration:show"
  },
  "dependencies": {
    "@tsed/ajv": "^7.27.1",
    "@tsed/common": "^7.27.1",
    "@tsed/components-scan": "^7.29.0",
    "@tsed/core": "^7.27.1",
    "@tsed/di": "^7.27.1",
    "@tsed/engines": "^7.27.1",
    "@tsed/exceptions": "^7.27.1",
    "@tsed/json-mapper": "^7.27.1",
    "@tsed/logger": "^6.5.0",
    "@tsed/logger-file": "^6.5.0",
    "@tsed/openspec": "^7.27.1",
    "@tsed/platform-cache": "^7.27.1",
    "@tsed/platform-exceptions": "^7.27.1",
    "@tsed/platform-express": "^7.27.1",
    "@tsed/platform-log-middleware": "^7.27.1",
    "@tsed/platform-middlewares": "^7.27.1",
    "@tsed/platform-params": "^7.27.1",
    "@tsed/platform-response-filter": "^7.27.1",
    "@tsed/platform-views": "^7.27.1",
    "@tsed/schema": "^7.27.1",
    "@tsed/swagger": "^7.27.1",
    "ajv": "^8.12.0",
    "barrelsby": "^2.5.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "method-override": "^3.0.0",
    "mysql": "^2.18.1",
    "typeorm": "^0.3.11",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "4.5.0",
    "@tsed/cli-plugin-jest": "4.5.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.2",
    "connect": "^3.7.0",
    "jest": "^29.5.0",
    "multer": "^1.4.5-lts.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  },
  "tsed": {
    "convention": "default",
    "architecture": "default",
    "packageManager": "yarn"
  }
}
